def check(from:(Int,Int),to:(Int,Int),gemPosition:(Int,Int)):Boolean  ={
    (from,to,gemPosition) match {
        case ((x,y),(x_next,y_next),(gem_x,gem_y)) if (( y < gem_y & x==gem_x & gem_y <= y_next) || (y==gem_y & x < gem_x & gem_x <= x_next)  ) =>
          true
        case _ => false
    }
}
