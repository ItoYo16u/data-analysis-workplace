def c(n:Int,d:Int=1,s:Set[Int]=Set()) :Set[Int] = {
    if(d>math.sqrt(n)){
        s
    }else {
        if(n%d==0){
            c(n,d+1,s++Set(d,n/d))
        }else {
            c(n,d+1,s)
        }
    }
}


