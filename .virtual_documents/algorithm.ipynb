def solution(heights:Seq[Int],leftEnd:Int=0,rightEnd:Int,max:Int):Int = {
    var _l = leftEnd
    var _r = rightEnd
    var _max = max
    if(_l < _r){
        val S = (_r - _l)* math.min(heights(_l),heights(_r))
       _max = math.max(max,S)
        if(heights(_l)>heights(_r)){
            _r -= 1
        }else{
            _l +=1
        }
    solution(heights,_l,_r,_max)
    }else{
        _max
    }
}


