}


def isUgly(n:Int)={
    if(n==0){
        n
    }else{
        val factors = Seq(2,3,5)
        var _n = n
        factors.foreach{f =>
          while (_n%f==0){
              _n = _n/f
          }
        }
        _n==1
    }
}


def det3x3(a:Array[Array[Double]]):Double= {
    var tmp:Double = 0.0
    for(i <- 0 to 2){
        tmp = tmp+ a(i%3)(0)*a((i+1)%3)(1)*a((i+2)%3)(2)
    }
    for(i <- 0 to 2){
        tmp = tmp - a((2-i)%3)(0)*a((4-i)%3)(1)*a((3-i)%3)(2)
    }
    tmp
}


