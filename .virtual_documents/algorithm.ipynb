def extractSubset(seq:Seq[Int]):Set[Seq[Int]]= {
        (0 to seq.length-1).flatMap{i =>
          (i to seq.length-1).foldLeft(Set[Seq[Int]]()){case (acc,j)=>
          
            acc+seq.slice(i,j+1)
          }
        }.toSet       
}



