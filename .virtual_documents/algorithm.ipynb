def upperTriangleMatrix(a:Array[Array[Double]]):Array[Array[Double]] = {
    val n = a.length
    
    for(i <- 0 to n-1){
        for(j <- 0 to n-1){
            if(i<j){
                val v = a(j)(i)/a(i)(i)
                for(k <- 0 to n-1){
                    a(j)(k) = a(j)(k)-a(i)(k)*v
                }
            }
        }
    }
    a  
}
