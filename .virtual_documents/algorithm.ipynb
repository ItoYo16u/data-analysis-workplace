def moveZeros(arr:Array[Int],readFrom:Int=0):Array[Int]={
    if(arr.length-1 < readFrom){
        return arr
    }else{
        if(arr(readFrom)!=0){
            moveZeros(arr,readFrom+1)
        }else{
            var i = 0
            while(readFrom+i<arr.length-1 & arr(readFrom+i)==0){
                i+=1
            }
            arr(readFrom) = arr(readFrom+i)
            arr(readFrom+i)=0
            moveZeros(arr,readFrom+1)
        }
    }

}
