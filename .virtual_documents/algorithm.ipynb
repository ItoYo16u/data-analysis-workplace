    schedules.foreach{schedule=>
        schedule.foreach{day=>
          if(candidates.contains(day)){
              candidates(day)+=1
              if(candidates(day)>=required){
                  return day
              }
          }else{
              candidates(day)=1
          }
        }
    }
    0
