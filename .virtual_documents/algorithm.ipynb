def action(arr:Array[Int],ops:(Int,Int))={
    val p = ops._1
    val c = ops._2
    val n = arr.length
    val up = arr.slice(n-p-c+1,n-p+1)
    for(i <- n-p-c+1 to n-1){
        if(i<=n-p+1 || i+c < n){
            if(i+c < n){
                arr(i)=arr(i+c)
            }
        }else{
            
            arr(i)=up(i-n+c)  
        }
    }         
}
